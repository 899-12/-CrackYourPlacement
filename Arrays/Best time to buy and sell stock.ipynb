   "source": [
    "class Solution:\n",
    "    def maxProfit(self, prices: List[int]) -> int:\n",
    "        buy_price=prices[0]\n",
    "        profit=0\n",
    "        for i in prices[1:]:\n",
    "            if buy_price>i:\n",
    "                buy_price=i\n",
    "            profit=max(profit,i-buy_price)\n",
    "        return profit\n",
    "    "
   ]
  }
